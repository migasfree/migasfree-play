{
  "name": "migasfree-play",
  "version": "5.10.0",
  "type": "module",
  "description": "Migasfree Client front-end. Allow install/uninstall available applications and devices.",
  "productName": "migasfree-play",
  "author": "Jose Antonio Chavarría <jachavar@gmail.com>",
  "license": "GPLv3",
  "copyright": "© 2017-2025 migasfree team",
  "homepage": "https://github.com/jact/migasfree-play/blob/master/README.md",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,ts,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,css,ts,cjs,mjs,html,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,vue,css,ts,cjs,mjs,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "gettext:extract": "vue-gettext-extract",
    "gettext:compile": "vue-gettext-compile",
    "dev": "quasar dev -m electron -- --no-sandbox --disable-gpu-sandbox",
    "build": "quasar build -m electron -- --no-sandbox --disable-gpu-sandbox"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "2.0.0",
    "@electron/remote": "2.1.2",
    "@quasar/extras": "1.16.17",
    "axios": "1.9.0",
    "compare-versions": "6.1.1",
    "cors": "2.8.5",
    "dayjs": "1.11.13",
    "express": "5.1.0",
    "express-rate-limit": "7.5.0",
    "html-to-text": "9.0.5",
    "pinia": "3.0.2",
    "python-shell": "5.0.0",
    "qrcode": "1.5.4",
    "quasar": "2.18.1",
    "tcp-port-used": "1.0.2",
    "typeface-lato": "1.1.13",
    "typeface-ubuntu-mono": "1.1.13",
    "util": "0.12.5",
    "vue": "3.5.13",
    "vue-router": "4.5.1",
    "vue3-gettext": "4.0.0-alpha.8"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/eslint-parser": "7.27.1",
    "@electron/packager": "18.3.6",
    "@quasar/app-webpack": "4.2.0",
    "@quasar/quasar-app-extension-qmarkdown": "2.0.5",
    "@vue/eslint-config-prettier": "10.2.0",
    "autoprefixer": "10.4.21",
    "babel-loader": "10.0.0",
    "devtron": "1.4.0",
    "electron": "35.7.5",
    "electron-builder": "26.0.12",
    "electron-debug": "4.1.0",
    "electron-devtools-installer": "4.0.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-vue": "9.32.0",
    "eslint-webpack-plugin": "5.0.1",
    "globals": "16.1.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "webpack": "5.99.8"
  },
  "resolutions": {
    "call-bind": "1.0.7",
    "get-intrinsic": "1.2.4"
  },
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "engines": {
    "node": ">= 20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
