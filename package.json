    "dayjs": "1.11.10",
    "express": "4.18.2",
    "express-rate-limit": "6.11.2",
    "html-to-text": "9.0.5",
    "pinia": "2.1.7",
    "python-shell": "5.0.0",
    "qrcode": "1.5.3",
    "quasar": "2.13.1",
    "tcp-port-used": "1.0.2",
    "typeface-lato": "1.1.13",
    "typeface-ubuntu-mono": "1.1.13",
    "util": "0.12.5",
    "vue": "3.3.4",
