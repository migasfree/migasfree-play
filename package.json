{
  "name": "migasfree-play",
  "version": "5.11.0",
  "type": "module",
  "description": "Migasfree Client desktop front-end. Allows install/uninstall available applications and devices.",
  "productName": "migasfree-play",
  "author": "Jose Antonio Chavarría <jachavar@gmail.com>",
  "license": "GPLv3",
  "copyright": "© 2017-2026 migasfree team",
  "homepage": "https://github.com/migasfree/migasfree-play",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,ts,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,css,ts,cjs,mjs,html,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,vue,css,ts,cjs,mjs,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "gettext:extract": "vue-gettext-extract",
    "gettext:compile": "vue-gettext-compile",
    "dev": "quasar dev -m electron -- --no-sandbox --disable-gpu-sandbox",
    "build": "quasar build -m electron -- --no-sandbox --disable-gpu-sandbox",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/lato": "5.2.6",
    "@fontsource/ubuntu-mono": "5.2.6",
    "@quasar/extras": "1.17.0",
    "axios": "1.13.5",
    "compare-versions": "6.1.1",
    "dayjs": "1.11.19",
    "html-to-text": "9.0.5",
    "pinia": "3.0.4",
    "python-shell": "5.0.0",
    "qrcode.vue": "3.6.0",
    "quasar": "2.18.6",
    "util": "0.12.5",
    "vue": "3.5.26",
    "vue-router": "4.6.4",
    "vue3-gettext": "4.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@electron/packager": "19.0.1",
    "@pinia/testing": "1.0.3",
    "@quasar/app-webpack": "4.3.1",
    "@quasar/quasar-app-extension-qmarkdown": "2.0.5",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "@vue/compiler-sfc": "3.5.26",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.23",
    "babel-loader": "10.0.0",
    "devtron": "1.4.0",
    "electron": "39.2.7",
    "electron-builder": "26.4.0",
    "electron-debug": "4.1.0",
    "electron-devtools-installer": "4.0.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-vue": "10.6.2",
    "eslint-webpack-plugin": "5.0.2",
    "globals": "17.0.0",
    "happy-dom": "20.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "vite": "7.3.1",
    "vitest": "4.0.16",
    "vue-eslint-parser": "10.2.0",
    "webpack": "5.104.1"
  },
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "engines": {
    "node": ">= 22.12.0",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "lint-staged": {
    "*.{js,vue,ts,cjs,mjs}": [
      "prettier --write",
      "eslint --fix",
      "vitest related --run"
    ],
    "*.{css,html,md,json}": [
      "prettier --write"
    ]
  }
}
