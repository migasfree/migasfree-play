{
  "name": "migasfree-play",
  "version": "2.0.0",
  "description": "Migasfree Client front-end. Allow install/uninstall available applications and printers.",
  "productName": "migasfree-play",
  "author": "Jose Antonio Chavarría <jachavar@gmail.com>",
  "license": "GPLv3",
  "copyright": "© 2017-2021 migasfree team",
  "homepage": "https://github.com/jact/migasfree-play/blob/master/README.md",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "gettext-extract": "gettext-extract --attribute v-translate --output src/i18n/app.pot $(find ./src -type f -name '*.vue' -o -name '*.js')",
    "gettext-compile": "gettext-compile --output src/i18n/translations.json src/i18n/*.po",
    "dev": "quasar dev -m electron -- --no-sandbox",
    "build": "quasar build -m electron -- --no-sandbox"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0-rc",
    "@electron/remote": "^1.2.0",
    "@jshmrtn/vue3-gettext": "^1.0.4",
    "@quasar/extras": "1.10.8",
    "axios": "^0.21.1",
    "core-js": "^3.15.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "python-shell": "^3.0.0",
    "qrcode": "^1.4.4",
    "quasar": "2.0.3",
    "tcp-port-used": "^1.0.2",
    "typeface-lato": "^1.1.13",
    "typeface-ubuntu-mono": "^1.1.13",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app": "3.0.3",
    "@quasar/quasar-app-extension-qmarkdown": "2.0.0-alpha.4",
    "devtron": "^1.4.0",
    "easygettext": "^2.17.0",
    "electron": "13.1.7",
    "electron-builder": "^22.4.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.1.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.7.0",
    "eslint-webpack-plugin": "^2.4.0"
  },
  "browserslist": [
    "chrome > 82"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}